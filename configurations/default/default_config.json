{
  "targets": {
    "os": "centos/8",
    "production_interface": "eth1",
    "servers": {
      "server1": {
        "memory": 6144
      },
      "server2": {
        "memory": 6144
      },
      "server3": {
        "memory": 6144
      },
      "server4": {
        "memory": 6144
      }
    }
  },
  "punch": {
    "zookeeper": {
      "clusters": {
        "zk_central": {
          "servers": [
            "server1",
            "server2",
            "server3",
            "server4"
          ]
        }
      }
    },
    "kafka": {
      "clusters": {
        "kafka_data": {
          "zk_cluster": "zk_central",
          "servers": [
            "server1",
            "server2",
            "server3"
          ]
        },
        "kafka_metrics": {
          "zk_cluster": "zk_central",
          "servers": [
            "server4"
          ]
        }
      }
    },
    "shiva": {
      "clusters": {
        "shiva_central": {
          "kafka_cluster": "kafka_metrics",
          "servers": [
            "server3",
            "server4"
          ]
        }
      }
    },
    "elasticsearch": {
      "clusters": {
        "es_data": {
          "servers": [
            "server1",
            "server2",
            "server3"
          ]
        },
        "es_metric": {
          "servers": [
            "server4"
          ]
        }
      }
    },
    "gateway": {
      "clusters": {
        "mytenant": {
          "servers": [
            "server2"
          ],
          "es_data_cluster": "es_data",
          "es_metric_cluster": "es_metric"
        }
      }
    },
    "kibana": {
      "domains": {
        "data-admin": {
          "server": "server1",
          "es_cluster": "es_data",
          "plugin_gateway": "mytenant"
        },
        "metric-admin": {
          "server": "server1",
          "es_cluster": "es_metric",
          "plugin_gateway": "mytenant"
        }
      }
    },
    "storm": {
      "clusters": {
        "storm_central": {
          "zk_cluster": "zk_central",
          "master_servers": [
            "server3"
          ],
          "slave_servers": [
            "server4"
          ],
          "ui_servers": [
            "server1"
          ]
        }
      }
    },
    "spark": {
      "clusters": {
        "spark_central": {
          "zk_cluster": "zk_central",
          "master_servers": [
            "server3"
          ],
          "slave_servers": [
            "server4"
          ]
        }
      }
    },
    "metricbeat": {
      "es_cluster": "es_metric"
    },
    "reporters": {
      "kafka_cluster": "kafka_metrics"
    }
  }
}
