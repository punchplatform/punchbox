{
  "targets": {
    "os": "centos",
    "production_interface": "eth1",
    "servers": {
      "central-front-1": {
        "memory": 6144
      },
      "central-front-2": {
        "memory": 6144
      },
      "central-back-1": {
        "memory": 6144
      },
      "central-back-2": {
        "memory": 6144
      }
    }
  },
  "punch": {
    "tls": true,
    "zookeeper": {
      "max_memory": "128m",
      "clusters": {
        "zk_central": {
          "servers": [
            "central-back-1",
            "central-back-2",
            "central-front-1",
            "central-front-2"
          ]
        }
      }
    },
    "kafka": {
      "clusters": {
        "kafka_data": {
          "zk_cluster": "zk_central",
          "servers": [
            "central-front-1",
            "central-front-2",
            "central-back-1"
          ]
        },
        "kafka_metric": {
          "zk_cluster": "zk_central",
          "servers": [
            "central-back-2"
          ]
        }
      }
    },
    "shiva": {
      "clusters": {
        "shiva_central": {
          "kafka_cluster_storage": "kafka_data",
          "servers": [
            "central-back-1",
            "central-back-2"
          ]
        }
      }
    },
    "elasticsearch": {
      "clusters": {
        "es_data": {
          "servers": [
            "central-front-1",
            "central-front-2",
            "central-back-1"
          ]
        },
        "es_metric": {
          "servers": [
            "central-back-2"
          ]
        }
      }
    },
    "gateway": {
      "clusters": {
        "skyblue": {
          "servers": [
            "central-back-2"
          ],
          "es_data_cluster": "es_data",
          "es_metric_cluster": "es_metric"
        }
      }
    },
    "kibana": {
      "domains":{
        "data_admin": {
          "server": "central-front-1",
          "es_cluster_target": "es_data",
          "plugin_gateway_cluster_target": "skyblue"
        },
        "metric_admin": {
          "server": "central-front-1",
          "es_cluster_target": "es_metric",
          "plugin_gateway_target_cluster": "skyblue"
        }
      }
    },
    "metricbeat": {
      "es_cluster_target": "es_metric"
    }
  }
}
