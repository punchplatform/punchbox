{
  "targets": {
    "os": "centos/8",
    "production_interface": "eth1",
    "servers": {
      "server1": {
        "memory": 6144
      },
      "server2": {
        "memory": 6144
      },
      "server3": {
        "memory": 6144
      },
      "server4": {
        "memory": 6144
      }
    }
  },
  "punch": {
    "tls": false,
    "reporters": {
      "kafka_cluster": "kafka_central"
    },
    "operators": {
      "kafka_cluster": "kafka_central",
      "servers": ["server1"]
    },
    "zookeeper": {
      "clusters": {
        "zk_central": {
          "servers": ["server3"]
        }
      }
    },
    "kafka": {
      "clusters": {
        "kafka_central": {
          "zk_cluster": "zk_central",
          "servers": ["server3"]
        }
      }
    },
    "elasticsearch": {
      "clusters": {
        "es_data": {
          "servers": ["server1"]
        },
        "es_metric": {
          "servers": ["server2"]
        }
      }
    },
    "gateway": {
      "clusters": {
        "mytenant": {
          "servers": ["server2"],
          "es_data_cluster": "es_data",
          "es_metric_cluster": "es_metric"
        }
      }
    },
    "kibana": {
      "domains": {
        "admin": {
          "server": "server1",
          "es_cluster": "es_data",
          "plugin_gateway": "mytenant"
        }
      }
    },
    "shiva": {
      "clusters": {
        "shiva_central": {
          "kafka_cluster": "kafka_central",
          "servers": ["server3"]
        }
      }
    }
  }
}
