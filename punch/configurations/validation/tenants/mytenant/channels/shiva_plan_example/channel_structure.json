{
  "version": "6.0",
  "start_by_tenant": false,
  "stop_by_tenant": true,
  "applications": [
    {
      "name": "simple-plan",
      "runtime": "shiva",
      "command": "planctl",
      "args": [
        "start",
        "--plan",
        "plan.hjson",
        "--template",
        "punchline.template",
        "--runtime",
        "spark"
      ],
      "cluster": "common",
      "shiva_runner_tags": [
        "common"
      ]
    }
  ]
}
